case -1:     LONGVARCHAR
case 0 :     NULL
case 1 :     CHAR
case 2 :     NUMERIC
case 3 :     DECIMAL
case 4 :     INTEGER
case 5 :     SMALLINT
case 6 :     FLOAT
case 7 :     REAL
